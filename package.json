{
  "name": "dcs-angular2-starter",
  "version": "1.0.0",
  "description": "Starting point for angular2 projects at DCS.",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run clean  && webpack --config config/webpack.dev.js  --progress --profile",
    "build:prod": "npm run clean:prod && npm run ngc && webpack --config config/webpack.prod.js --progress --profile --bail",
    "watch:dev": "npm run clean && webpack-dashboard -- webpack-dev-server --config config/webpack.dev.js  --profile --watch --inline --content-base src/",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "serve:prod": "http-server build/production --gzip -p 8080 -c-1",
    "serve:prod:ssl": "http-server build/production --gzip -p 443 -c-1 -S -C config/server.crt -K config/server.key",
    "deploy:prepare": "npm run nsp && npm run lint && npm test && npm run build:prod",
    "test": "karma start",
    "test:watch": "karma start --singleRun=false --autoWatch=true --report-slower-than=100",
    "protractor:update": "webdriver-manager update",
    "protractor:selenium": "webdriver-manager start",
    "protractor:run": "LANG='en_US.UTF-8' protractor protractor.conf.js",
    "lint": "tslint 'src/**/*.ts'",
    "docs": "typedoc --out docs --ignoreCompilerErrors --exclude '**/*spec.ts' --exclude '**/*ngfactory.ts' --mode file ./src/app",
    "backend:mock": "json-server mock/db.json --port 3001",
    "clean": "rimraf ./build",
    "clean:prod": "rimraf ./build/production",
    "webpack": "webpack-dashboard",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "nsp": "nsp check"
  },
  "config": {
    "save-exact": true
  },
  "repository": {
    "type": "git",
    "url": "https://DcsMarcRemolt@bitbucket.org/DcsMarcRemolt/dcs-angular2-starter.git"
  },
  "keywords": [
    "angular2",
    "blueprint",
    "rxjs",
    "redux",
    "webpack"
  ],
  "author": "Marc Remolt <marc.remolt@dcs-fuerth.de> (http://www.dcs-fuerth.de)",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
